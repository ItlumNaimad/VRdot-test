render_mode blend_mix, depth_draw_opaque, cull_back, diffuse_lambert, specular_schlick_ggx;

uniform sampler2D noise0;
uniform sampler2D noise1;
uniform sampler2D noise2;

vec3 psin(float t, vec3 low, vec3 high) {
	return low + (sin(t) + 1.0) * (high - low) / 2.0;
}

void vertex() {
	UV *= 0.4;
}

void fragment() {
	vec2 uv0 = UV + vec2(mod(TIME * 0.1, 1.0), mod(TIME * 0.11, 1.0));
	vec2 uv1 = UV + vec2(mod(TIME * 0.08, 1.0), mod(TIME * 0.18, 1.0));
	vec2 uv2 = UV + vec2(mod(TIME * 0.13, 1.0), mod(TIME * 0.10, 1.0));
	ALBEDO = vec3(0.02, 0.02, 0.02);
	EMISSION =
		psin(TIME, vec3(0.0), vec3(0.8, 0.6, 0.0) * texture(noise0, uv0).rgb) +
		psin(TIME * 0.7, vec3(0.0), vec3(0.03, 0.01, 0.03) * texture(noise1, uv1).rgb) +
		psin(TIME * 0.4, vec3(0.0), vec3(0.01, 0.03, 0.13) * texture(noise2, uv2).rgb);
}
"

